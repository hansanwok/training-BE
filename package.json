{
  "name": "training-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "nodemon",
    "dev": "npm-run-all server",
    "server": "DEBUG=training-be:* babel-node ./bin/www",
    "clean": "rimraf dist",
    "start": "npm run prod",
    "server:prod": "node ./dist/bin/www",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0"
  }
}
